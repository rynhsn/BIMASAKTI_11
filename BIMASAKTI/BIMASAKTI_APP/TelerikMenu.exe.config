<?xml version="1.0"?>
<configuration>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <system.net>
    <connectionManagement>
      <add maxconnection="200" address="*" />
    </connectionManagement>
  </system.net>
  <system.serviceModel>
    <client>
      <endpoint address="http://192.168.10.254/LDH_WEB_KEY_IN/RINTKEYWebService.asmx"
        binding="basicHttpBinding" bindingConfiguration="RINTKEYWebServiceSoap"
        contract="KeyServiceRef.RINTKEYWebServiceSoap" name="RINTKEYWebServiceSoap" />
      <endpoint address="http://localhost:64530/Services/MenuService/MenuService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IMenuService"
        contract="MenuServiceRef.IMenuService" name="BasicHttpBinding_IMenuService" />
      <endpoint address="http://localhost:64530/Services/LoginService/LoginService.svc"
