        contract="LoginServiceRef.ILoginService" name="BasicHttpBinding_ILoginService" />
      <endpoint address="http://localhost:64530/Services/MenuService/MenuStreamingService/MenuStreamingService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IMenuStreamingService"
        contract="MenuStreamingServiceRef.IMenuStreamingService" name="BasicHttpBinding_IMenuStreamingService" />
    </client>
    <extensions>
      <behaviorExtensions>
        <add name="ServiceContext" type="R_Common.R_ServiceContextBehavior, REL_Common"/>
      </behaviorExtensions>
    </extensions>
    <behaviors>
      <endpointBehaviors>
        <behavior name="DefaultEndPointBehavior">
          <ServiceContext/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <bindings>
      <basicHttpBinding>
        <binding name="StreamingBinding" receiveTimeout="00:10:00" sendTimeout="00:10:00"
          maxReceivedMessageSize="9223372036854775807" transferMode="StreamedResponse">
          <readerQuotas maxArrayLength="50000000" />
          <security mode="None" />
        </binding>
        <binding name="RegularBinding" receiveTimeout="00:10:00" sendTimeout="00:10:00"
          maxReceivedMessageSize="50000000" messageEncoding="Mtom">
          <readerQuotas maxArrayLength="50000000" />
          <security mode="None" />
        </binding>
        <binding name="RINTKEYWebServiceSoap" />
        <binding name="BasicHttpBinding_IMenuService" messageEncoding="Mtom" />
        <binding name="BasicHttpBinding_ILoginService" messageEncoding="Mtom" />
        <binding name="BasicHttpBinding_IMenuStreamingService" />
      </basicHttpBinding>
    </bindings>
  </system.serviceModel>
