            <R_ItemLayout col="7">
                <R_TextBox Value="@_viewModel.Entity.CBUILDING_NAME" Enabled="false"/>
            </R_ItemLayout>
        </R_StackLayout>
        <R_StackLayout Row>
            <R_ItemLayout col="2">
                <R_Label>@_localizer["LBL_FLOOR"]</R_Label>
            </R_ItemLayout>
            <R_ItemLayout col="3">
                <R_TextBox Value="@_viewModel.Entity.CFLOOR_ID" Enabled="false"/>
