        <R_StackLayout Row>
            <R_ItemLayout col="2">
                <R_Label>@_localizer["LBL_BUILDING"]</R_Label>
            </R_ItemLayout>
            <R_ItemLayout col="3">
                <R_TextBox Value="@_viewModel.Entity.CBUILDING_ID" Enabled="false"/>
